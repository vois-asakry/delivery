+}
+
+class _PrivacyTermScreenState extends State<PrivacyTermScreen> {
+  void _onTapAgree() {
+    injector<SharedPreferences>().setBool(LocalStorageKey.agreePrivacy, true);
+    Navigator.of(context).pushReplacementNamed(RouteList.dashboard);
+  }
